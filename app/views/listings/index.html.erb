<h3>Please choose your rental criteria below</h3>

<%= form_for @listing do |f| %>

<%= f.label :beds %>
<%= f.text_field :beds %>
<br>

<%= f.label :baths %>
<%= f.text_field :baths %>
<br>

<%= f.label :min_price %>
<%= f.text_field :min_price %>
<br>

<%= f.label :max_price %>
<%= f.text_field :max_price %>
<br>

<%= f.submit %>

<% end %>